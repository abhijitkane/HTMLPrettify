{
  "manifest_version": 2,

  "name": "HTMLPrettify",
  "description": "This extension indents HTML code.",
  "version": "1.0",

  "app":{
        "launch":{
            "local_path":"index.htm"
        }
  },

  "permissions": [
    "storage"
  ],

  "content_scripts": [ {"js": [ "js/main.js","js/eventBindings.js","index.htm" ], "matches": [ "http://*/" ]} ]
 
}